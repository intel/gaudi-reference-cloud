  computeBm:
    enabled: true
  computeVm:
    enabled: true
  dpai:
    enabled: false
  firewall:
    enabled: false
  fleetAdmin:
    enabled: false
  training:
    enabled: false
    armada:
      enabled: false
  cloudmonitor:
    enabled: false
  cloudmonitorLogs:
    enabled: false
  iks:
    enabled: true
    listenPort: 8083
  iksDb:
    enabled: false
    primary:
        persistence:
          enabled: false
  iksOperators:
    enabled: false
  maasCompute:
    enabled: false
  insights:
    enabled: false
  insightsDb:
    enabled: false
    primary:
        persistence:
          enabled: false
  rateLimit:
    enabled: false
  sdnVN:
    enabled: false
  storageFile:
    enabled: false
  storageObject:
    enabled: false
  loadbalancer:
    enabled: false
  network:
    enabled: false
  networkCluster:
    enabled: false # Controls whether to deploy the services in helmfile-network-cluster (to a separate cluster, or to one of the existing clusters depending on availabilityZone.networkCluster.kubecontext)
  kfaas:
    enabled: false
    listenPort: 8083
  dataloader:
    enabled: false
  maas:
    enabled: false
  pgOperator:
    enabled: false
  quickConnect:
    enabled: false
  minioOperator:
    enabled: false
  minioTenant:
    enabled: false
  quotaManagementService:
    enabled: false
  tftpServer:
    enabled: false
global:
