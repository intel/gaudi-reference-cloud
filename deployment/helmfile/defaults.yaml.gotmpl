log:
  # Zap log encoding (one of 'json' or 'console'). Use json with ELK.
  encoder: json
  # Zap Level to configure the verbosity of logging. Can be one of 'debug', 'info', 'error', or any integer value > 0 which corresponds to custom debug levels of increasing verbosity.
  level: debug
  # Zap Level at and above which stacktraces are captured (one of 'info', 'error', 'panic')
  stacktraceLevel: error
  # gRPC log severity level. Can be one of 'error', 'warning', or 'info'.
  grpcGoLogSeverityLevel: error
  # gRPC log verbosity level. Higher numbers will result in more logs.
  grpcGoLogVerbosityLevel: 0
  # gRPC log encoding format. options: json, console (default:console)
