      commit: HEAD
      enabled: false
    authz:
      commit: HEAD
      enabled: true
    billing:
      commit: HEAD
      enabled: true
    cloudaccount:
      commit: HEAD
      enabled: true
    cloudcredits:
      commit: HEAD
      enabled: true
    cloudmonitor:
      commit: HEAD
      enabled: false
    externalSecrets:
      commit: HEAD
      enabled: true
    grpc:
      commit: HEAD
      enabled: true
    metering:
      commit: HEAD
      enabled: true
    oidc:
      commit: HEAD
      enabled: true
    portal:
      commit: HEAD
      enabled: true
    populateProductCatalog:
      commit: HEAD
      enabled: false
    productCatalog:
      commit: HEAD
      enabled: true
    rateLimit:
      commit: HEAD
