# Ansible generated ~/.ssh/config for the Bastion server

{% for vmnum in range(1, guest_test_host_deployments|int + 1) %}
Host guest-{{ vmnum }} guest-test-{{ vmnum }} 192.168.{{ vmnum + 100 }}.2
    User sdp
    HostName 192.168.{{ vmnum + 100 }}.2
    IdentityFile ~/.ssh/guest-test-{{ vmnum }}

{% endfor %}
